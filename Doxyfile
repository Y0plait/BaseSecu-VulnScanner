# Doxyfile 1.9.x
# Project: VisualNet Scanner - Network Vulnerability Assessment Tool
# Description: Vulnerability scanner for Linux machines with NVD integration

#---------------------------------------------------------------------------
# Project related configuration options
#---------------------------------------------------------------------------

PROJECT_NAME           = "VisualNet Scanner"
PROJECT_NUMBER         = 1.0
PROJECT_BRIEF          = "Comprehensive vulnerability scanner for Linux machines with network visualization"
PROJECT_LOGO           = 

# The OUTPUT_DIRECTORY tag is used to specify the (relative or absolute) 
# base path where the generated documentation will be put.
OUTPUT_DIRECTORY       = ./docs/doxygen

# The INPUT tag can be used to specify the files and/or directories that contain 
# documented source files.
INPUT                  = src/ main.py README.md

# File patterns to process
FILE_PATTERNS          = *.py *.md

# Recursive input
RECURSIVE              = YES

# Exclude patterns
EXCLUDE_PATTERNS       = */.*/* */__pycache__/* *.pyc

#---------------------------------------------------------------------------
# Build related configuration options
#---------------------------------------------------------------------------

# The EXTRACT_ALL tag can be used to get todo list populated with all items.
EXTRACT_ALL            = NO

# If the EXTRACT_PRIVATE tag is set to YES, all private members of a class 
# will be included in the documentation.
EXTRACT_PRIVATE        = NO

# If the EXTRACT_PACKAGE tag is set to YES, all members with package or internal 
# scope will be included in the documentation.
EXTRACT_PACKAGE        = NO

# If the EXTRACT_STATIC tag is set to YES all static members of a file will be 
# included in the documentation.
EXTRACT_STATIC         = NO

# This flag is only useful for Objective-C projects. When set to YES local classes 
# (classes defined in implementation files) are included in the documentation.
EXTRACT_LOCAL_CLASSES  = NO

# Set the SHOW_NAMESPACES tag to NO to hide the namespaces.
SHOW_NAMESPACES        = YES

#---------------------------------------------------------------------------
# Configuration options related to the input files
#---------------------------------------------------------------------------

# If the value of the INPUT tag contains directories, you can use the 
# FILE_PATTERNS tag to specify one or more wildcard patterns
USE_MDFILE_AS_MAINPAGE = README.md

# The FULL_PATH_NAMES tag controls whether the full path (if available) of input 
# files is shown or just the file name.
FULL_PATH_NAMES        = NO

# The STRIP_FROM_PATH tag can be used to strip a user-defined part of the path.
STRIP_FROM_PATH        = /Users/amoulin/SynologyDrive/Etudes/ICS/Cours/IntroCybersecu/ProjetFinal/

# If left blank the directory in which the Doxyfile is located is used.
STRIP_FROM_INC_PATH    = 

#---------------------------------------------------------------------------
# Configuration options related to source browsing
#---------------------------------------------------------------------------

# If the SOURCE_BROWSER tag is set to YES then a list of source files will 
# be generated. Documented entities will be cross-referenced with these sources.
SOURCE_BROWSER         = YES

# Setting the INLINE_SOURCES tag to YES will include the body of functions, 
# classes and enums directly into the documentation.
INLINE_SOURCES         = NO

# Setting STRIP_CODE_COMMENTS to NO will instruct doxygen not to filter out 
# the special comment blocks that are used to separate sections of the code.
STRIP_CODE_COMMENTS    = YES

#---------------------------------------------------------------------------
# Configuration options related to the alphabetical class index
#---------------------------------------------------------------------------

# If the ALPHABETICAL_INDEX tag is set to YES, an alphabetical index of all 
# compounds will be generated.
ALPHABETICAL_INDEX     = YES

# The COLS_IN_ALPHA_INDEX tag determines the number of columns in which the 
# alphabetical index list will be split
COLS_IN_ALPHA_INDEX    = 5

#---------------------------------------------------------------------------
# Configuration options related to the HTML output
#---------------------------------------------------------------------------

# If the GENERATE_HTML tag is set to YES Doxygen will generate HTML output
GENERATE_HTML          = YES

# The HTML_OUTPUT tag is used to specify where the HTML docs will be put.
HTML_OUTPUT            = html

# The HTML_FILE_EXTENSION tag can be used to specify the file extension for 
# each generated HTML page.
HTML_FILE_EXTENSION    = .html

# The HTML_HEADER tag can be used to specify a user-defined HTML header file
# for each generated HTML page.
HTML_HEADER            = 

# The HTML_FOOTER tag can be used to specify a user-defined HTML footer for 
# each generated HTML page.
HTML_FOOTER            = 

# The HTML_STYLESHEET tag can be used to specify a user-defined cascading style 
# sheet that is used by each HTML page.
HTML_STYLESHEET        = 

# The HTML_COLORSTYLE_HUE tag controls the color of the HTML output.
HTML_COLORSTYLE_HUE    = 220

# The HTML_COLORSTYLE_SAT tag controls the color saturation of the HTML output.
HTML_COLORSTYLE_SAT    = 100

# The HTML_COLORSTYLE_GAMMA tag controls the gamma correction of the color.
HTML_COLORSTYLE_GAMMA  = 80

# If the HTML_TIMESTAMP tag is set to YES then the footer of each generated HTML 
# page will contain the date and time when the page was generated.
HTML_TIMESTAMP         = YES

# If the HTML_DYNAMIC_SECTIONS tag is set to YES then the generated HTML 
# documentation will contain sections that can be hidden and shown after loading 
# the page as JavaScript.
HTML_DYNAMIC_SECTIONS  = YES

# With HTML_INDEX_NUM_ENTRIES one can control the preferred number of entries shown 
# in the various tree structured indices initially; the user can expand and collapse 
# lists dynamically anyway.
HTML_INDEX_NUM_ENTRIES = 100

# Use this tag to change the font size of LaTeX formulas included as images 
# in the HTML documentation.
FORMULA_FONTSIZE       = 10

# Use the FORMULA_TRANSPARENT tag to determine whether or not the images 
# generated for formulas are transparent PNGs.
FORMULA_TRANSPARENT    = YES

# Enable the USE_MATHJAX option to render LaTeX formulas using MathJax 
# (see http://www.mathjax.org) which is much more aesthetically pleasing.
USE_MATHJAX            = NO

#---------------------------------------------------------------------------
# Configuration options related to the LaTeX output
#---------------------------------------------------------------------------

# If the GENERATE_LATEX tag is set to YES Doxygen will generate Latex output.
GENERATE_LATEX         = NO

#---------------------------------------------------------------------------
# Configuration options related to the RTF output
#---------------------------------------------------------------------------

# If the GENERATE_RTF tag is set to YES Doxygen will generate RTF output 
GENERATE_RTF           = NO

#---------------------------------------------------------------------------
# Configuration options related to the man page output
#---------------------------------------------------------------------------

# If the GENERATE_MAN tag is set to YES Doxygen will generate man pages
GENERATE_MAN           = NO

#---------------------------------------------------------------------------
# Configuration options related to the XML output
#---------------------------------------------------------------------------

# If the GENERATE_XML tag is set to YES Doxygen will generate an XML file
GENERATE_XML           = NO

#---------------------------------------------------------------------------
# Configuration options for the AutoGen Definitions output
#---------------------------------------------------------------------------

# If the GENERATE_AUTOGEN_DEF tag is set to YES Doxygen will generate an AutoGen 
# Definitions (see autogen.sf.net) file.
GENERATE_AUTOGEN_DEF   = NO

#---------------------------------------------------------------------------
# Configuration options related to the Perl module output
#---------------------------------------------------------------------------

# If the GENERATE_PERLMOD tag is set to YES Doxygen will generate a Perl module 
# file.
GENERATE_PERLMOD       = NO

#---------------------------------------------------------------------------
# Configuration options related to the preprocessor
#---------------------------------------------------------------------------

# If the ENABLE_PREPROCESSING tag is set to YES Doxygen will evaluate all 
# C-preprocessor directives found in the sources and include files.
ENABLE_PREPROCESSING   = YES

# If the MACRO_EXPANSION tag is set to YES Doxygen will expand all macro names 
# in the source code.
MACRO_EXPANSION        = NO

#---------------------------------------------------------------------------
# Configuration options related to the dot tool
#---------------------------------------------------------------------------

# If you set the HAVE_DOT tag to YES then doxygen will assume the dot tool is 
# available from the path.
HAVE_DOT               = NO

# If the CLASS_GRAPH tag is set to YES then the inheritance hierarchy will be 
# shown in a graph.
CLASS_GRAPH            = YES

# If the COLLABORATION_GRAPH tag is set to YES then a graph showing the 
# inter-dependencies of the parsed files will be shown.
COLLABORATION_GRAPH    = YES

# If the GROUP_GRAPHS tag is set to YES then a graph showing the 
# dependencies between groups of files is generated as well.
GROUP_GRAPHS           = YES

# If the UML_LOOK tag is set to YES a graph showing the template relations 
# between classes is generated as well.
UML_LOOK               = NO

# If the UML_LOOK tag is enabled, the fields and methods are shown inside the 
# class node.
TEMPLATE_RELATIONS     = NO

# If the INCLUDE_GRAPH, ENABLE_PREPROCESSING and SEARCH_INCLUDES tags are set to 
# YES then doxygen will generate a graph for each documented file showing the 
# direct and indirect include dependencies of the file with other documented files.
INCLUDE_GRAPH          = YES

# If the INCLUDED_BY_GRAPH, ENABLE_PREPROCESSING and SEARCH_INCLUDES tags are set 
# to YES then doxygen will generate a graph showing the direct and indirect include 
# dependencies of the file with other documented files.
INCLUDED_BY_GRAPH      = YES

# If the CALL_GRAPH tag is set to YES then doxygen will generate a call 
# dependency graph for every documented function or method.
CALL_GRAPH             = NO

# If the CALLER_GRAPH tag is set to YES then doxygen will generate a caller 
# dependency graph for every documented function or method.
CALLER_GRAPH           = NO

#---------------------------------------------------------------------------
# Configuration options related to the search engine
#---------------------------------------------------------------------------

# If the SEARCHENGINE tag is set to YES the search engine will be enabled.
SEARCHENGINE           = YES

# When the SERVER_BASED_SEARCH tag is enabled the search engine will be 
# implemented using a web server instead of a web client using Javascript.
SERVER_BASED_SEARCH    = NO

#---------------------------------------------------------------------------
# Aliases for custom tags
#---------------------------------------------------------------------------

ALIASES                = "author=@author" \
                         "date=@date" \
                         "version=@version" \
                         "deprecated=@deprecated" \
                         "todo=@todo" \
                         "warning=@warning" \
                         "note=@note" \
                         "see=@see" \
                         "code{n}=@code{n}" \
                         "endcode=@endcode"
